{
  "name": "gizmo-web-lib",
  "version": "0.0.1",
  "description": "library of gizmo web client",
  "main": "index.js",
  "author": "Yoshiyuki Kato",
  "license": "MIT",
  "scripts": {
    "watch": "$(npm bin)/webpack --config webpack.config.js --watch --debug",
    "build" : "$(npm bin)/webpack --config webpack.config.js",
    "test": "$(npm bin)/nyc $(npm bin)/mocha test/**/*.test.ts",
    "server": "$(npm bin)/webpack-dev-server --config webpack.config.js --watch --debug"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "espower-typescript/guess"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  
  "devDependencies": {
    "@types/bluebird": "^3.5.4",
    "@types/jquery": "^2.0.45",
    "@types/jsdom": "^2.0.31",
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "@types/sinon": "^2.2.2",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.1",
    "comment-json": "^1.1.3",
    "css-loader": "^0.28.0",
    "espower-source": "^2.2.0",
    "espower-typescript": "^8.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.2",
    "nyc": "^11.0.2",
    "power-assert": "^1.4.2",
    "remap-istanbul": "^0.9.5",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.1.0",
    "typescript": "^2.3.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-iconv-plugin": "^0.0.2"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "jquery": "^3.2.1",
    "node-fs-extra": "^0.8.1",
    "react": "^15.4.2",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.4.2"
  }
}
